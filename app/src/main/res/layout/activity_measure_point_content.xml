<?xml version="1.0" encoding="utf-8"?>
<com.emms.ui.CustomDrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:drop="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:id="@+id/search_page"
    android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<RelativeLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/toolbar"
        android:id="@+id/toolbar"/>
    <TextView
        android:id="@+id/measure_point_id_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_below="@+id/toolbar"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="36dp"
        android:text="@string/measure_point_id"
        android:layout_toRightOf="@+id/sequence_number"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_alignTop="@+id/measure_point_id_tag"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/measure_point_id_tag"
        android:textSize="18sp"/>
    <TextView
    android:id="@+id/measure_point_name_tag"
    android:layout_below="@+id/measure_point_id"
    android:layout_marginTop="25dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignLeft="@+id/measure_point_id_tag"
    android:textColor="@color/text_color"
    android:text="@string/measure_point_name"
    android:layout_marginBottom="6dp"
    android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_alignTop="@+id/measure_point_name_tag"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/measure_point_name_tag"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_type_tag"
        android:layout_below="@+id/measure_point_name"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/measure_point_id_tag"
        android:textColor="@color/text_color"
        android:text="@string/measure_point_type"
        android:layout_marginBottom="6dp"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_alignTop="@+id/measure_point_type_tag"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/measure_point_type_tag"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_unit_tag"
        android:layout_below="@+id/measure_point_type"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/measure_point_id_tag"
        android:textColor="@color/text_color"
        android:text="@string/measure_point_unit"
        android:layout_marginBottom="6dp"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_unit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_alignTop="@+id/measure_point_unit_tag"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/measure_point_unit_tag"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/measure_point_range_tag"
        android:layout_below="@+id/measure_point_unit"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/measure_point_id_tag"
        android:textColor="@color/text_color"
        android:text="@string/measure_point_range"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/measure_point_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:layout_alignTop="@+id/measure_point_range_tag"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/measure_point_range_tag"
        android:textSize="18sp"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/measure_point_range_tag"
        android:layout_marginTop="15dp"
        android:id="@+id/lala"
        android:layout_below="@+id/measure_point_range">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:id="@+id/measure_point_result_tag"
        android:layout_centerVertical="true"
        android:textColor="@color/text_color"
        android:text="@string/measure_point_result"/>
        <!--<com.emms.ui.DropEditText-->
            <!--android:id="@+id/measure_point_result"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginRight="40dp"-->
            <!--android:layout_toRightOf="@+id/measure_point_result_tag"-->
            <!--drop:drawableRight="@mipmap/drop_down"-->
            <!--drop:dropMode="flow_parent"-->
            <!--drop:hint="@string/pleaseInput"-->
            <!--drop:hintColor="@color/hint_color" />-->
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="40dp"
            android:background="@drawable/bg_edit_normal"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:paddingLeft="5dp"
            android:hint="@string/pleaseInput"
            android:layout_toRightOf="@+id/measure_point_result_tag"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/lala"
        android:layout_marginTop="20dp">
        <Button
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#FCC900"
            android:gravity="center"
            android:id="@+id/comfirm"
            android:text="@string/sure"
            android:textSize="23sp"
            android:layout_alignParentBottom="true"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="20dp"
            />
    </RelativeLayout>
</RelativeLayout>
        </FrameLayout>
    <include layout="@layout/activity_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"/>
    </com.emms.ui.CustomDrawerLayout>