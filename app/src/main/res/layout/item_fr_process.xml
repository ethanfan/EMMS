            android:textSize="17sp"/>
        <TextView
            android:id="@+id/group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_color"
            android:paddingLeft="5dp"
            android:layout_toRightOf="@id/id_group"
            android:text="D"
            android:textSize="17sp"/>
